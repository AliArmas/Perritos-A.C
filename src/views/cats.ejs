<!DOCTYPE html>
<html lang="en">
<%- include('../views/modules/head'); %>
  <body>
    <%- include('../views/modules/nvarAd'); %>
      <div class="container">
        <div class="mt-5">
          <h3>Usuarios regitrados</h3>
          <div>
            <table class="table align-items-center">
              <thead>
                <tr>
                  <th scope="col">#</th>
                  <th scope="col">Nombre</th>
                  <th scope="col">Telefono</th>
                  <th scope="col">Email</th>
                  <th scope="col">Plan</th>
                </tr>
              </thead>
              <tbody>
                <% for (var i = 0; i < results.length; i++  ) { %>
                  <tr>
                    <th scope="row">
                      <%= results[i].idMascota %>
                    </th>
                    <td>
                      <%= results[i].nombreM %>
                    </td>
                    <td>
                      <%= results[i].tipoMascota %>
                    </td>
                    <td>
                      <%= results[i].raza %>
                    </td>
                    <td>
                      <%= results[i].peso %>
                    </td>
                    <td>
                      <%= results[i].estatura%>
                    <td>
                      <%= results[i].peso %>
                    </td>
                    <td>
                      <%= results[i].status %>
                    </td>

                    <td>
                      <%= results[i].date %>
                    </td>
                  </tr>
                  <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
  </body>
</html>